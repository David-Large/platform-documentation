{
  "data_reference": {
    "snippet": "<comp.DataReference>[[rows]]</comp.DataReference>",
    "inline": false,
    "preview": {
      "text": "Data Reference Table"
    },
    "params": {
      "rows": {
        "parser": "repeating",
        "options": {
          "editor_key": "rows",
          "snippet": "<comp.DataReferenceRow [[row_args]]>[[content]]</comp.DataReferenceRow>",
          "style": {
            "between": "\n",
            "block": {
              "indent": "  "
            }
          }
        }
      },
      "row_args": {
        "parser": "key_values",
        "options": {
          "models": [
            {
              "editor_key": "label",
              "type": "string"
            },
            {
              "editor_key": "type_markdown",
              "type": "string"
            }
          ],
          "format": {
            "ref": "mdx_format"
          }
        }
      },
      "content": {
        "parser": "content",
        "options": {
          "editor_key": "inner_markdown",
          "style": {
            "output": "block",
            "block": {
              "leading": "\n",
              "trailing": "\n",
              "indent": "  "
            }
          }
        }
      }
    }
  }
}