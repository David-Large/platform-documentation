{
  "multi_code_block": {
    "snippet": "<comp.MultiCodeBlock [[args]]>\n```\n[[inner]]\n```\n[[annotations]]</comp.MultiCodeBlock>",
    "inline": false,
    "preview": {
      "text": [
        {
          "key": "source"
        },
        "Multi Code Block"
      ],
      "subtext": [
        {
          "key": "content"
        }
      ],
      "icon": "data_object"
    },
    "_inputs": {
      "content": {
        "type": "code"
      }
    },
    "params": {
      "args": {
        "parser": "key_values",
        "options": {
          "models": [
            {
              "editor_key": "language",
              "type": "string"
            },
            {
              "editor_key": "translate_into",
              "type": "array",
              "default": [
                "json"
              ]
            },
            {
              "editor_key": "source",
              "type": "string"
            }
          ],
          "format": {
            "ref": "mdx_format"
          }
        }
      },
      "inner": {
        "parser": "content",
        "options": {
          "editor_key": "content",
          "allow_nested": false,
          "forbidden_tokens": [
            "```"
          ]
        }
      },
      "annotations": {
        "parser": "optional",
        "options": {
          "snippet": "[[annotation_repeat]]"
        }
      },
      "annotation_repeat": {
        "parser": "repeating",
        "options": {
          "default_length": 0,
          "editor_key": "annotations",
          "snippet": "<comp.Annotation [[annotation_args]]>[[annotation_content]]</comp.Annotation>",
          "style": {
            "between": "\n",
            "block": {
              "leading": "",
              "trailing": "\n"
            }
          }
        }
      },
      "annotation_args": {
        "parser": "key_values",
        "options": {
          "models": [
            {
              "editor_key": "number",
              "type": "string"
            }
          ],
          "format": {
            "ref": "mdx_format"
          }
        }
      },
      "annotation_content": {
        "parser": "content",
        "options": {
          "editor_key": "annotation_content",
          "allow_nested": false,
          "style": {
            "output": "block",
            "block": {
              "leading": "\n",
              "trailing": "\n",
              "indent": "  "
            }
          }
        }
      }
    }
  }
}